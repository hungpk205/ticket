<div class="div-floor">
  <p class="title"><%= t(".floor_1") %></p>
  <ul class="no-style-list-ul">
    <li id="floor1-A" class="li-row-floor">
      <ul class="no-style-list-ul">
        <% @tickets_t1.each do |ticket| %>
          <% if ticket.code.include? "A" %>
            <li class="seat-bus-row seat-<%= ticket.status %>">
              <div class="content-seat" style="text-align:left">
                <div class="name-seat">Ghế: <%= ticket.code[3,ticket.code.bytesize] %></div>
                <div>KH: <%= ticket.booking.present? && ticket.booking.paid? && ticket.booked? ? ticket.booking.fullname : "...................................................." %></div>
                <div>SĐT: <%= ticket.booking.present? && ticket.booking.paid? && ticket.booked? ? ticket.booking.phone : "...................................................." %></div>
                <div>Điểm dừng: ......................................</div>
              </div>
            </li>
          <% end %>
        <% end %>
      </ul>
    </li>
