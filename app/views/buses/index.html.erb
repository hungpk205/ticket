<div class="right_col" role="main">
  <div class="page-title">
    <div class="title_left">
      <h3><%= t(".bus") %></h3>
    </div>
    <div class="title_right">
      <div class="col-md-5 col-sm-5  form-group pull-right top_search">
        <div class="input-group">
          <input type="text" class="form-control" placeholder="<%= t("search") %>">
          <span class="input-group-btn">
            <button class="btn btn-default" type="button"><%= t("find") %></button>
          </span>
        </div>
      </div>
    </div>
  </div>
  <div class="clearfix"></div>
  <div class="row">
    <div class="col-md-8 col-sm-8 col-xs-8 offset-md-2">
      <div class="x_panel">
        <div class="x_title">
          <ul class="nav navbar-right panel_toolbox">
            <li>
              <%= button_to t(".add"), new_company_bus_path, method: :get, class: "btn btn-success btn-xs" %>
            </li>
          </ul>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">
          <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
              <thead>
                <tr class="headings">
                  <th class="column-title"><%= t(".license_plate") %> </th>
                  <th class="column-title"><%= t(".type_quality") %> </th>
                  <th class="column-title"><%= t(".slot") %> </th>
                  <th class="column-title"><%= t(".status") %> </th>
                  <th class="column-title no-link last">
                    <span class="nobr"><%= t("action") %></span>
                  </th>
                </tr>
              </thead>
              <tbody>
                <%= render @buses %>
              </tbody>
            </table>
            <%= paginate @buses %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
