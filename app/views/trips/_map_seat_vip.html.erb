<ul class="no-style-list-ul">
  <li id="ticket-floor1">
    <p><%= t(".floor_1") %></p>
    <ul class="no-style-list-ul">
      <li id="floor1-A" class="li-row-floor">
        <ul class="no-style-list-ul">
          <% @tickets_t1.each do |ticket| %>
            <% if ticket.code.include? "A" %>
              <li class="seat-bus-row seat-<%= ticket.status %>"><span><%= ticket.code[3,ticket.code.bytesize] %></span></li>
            <% end %>
          <% end %>
        </ul>
      </li>
